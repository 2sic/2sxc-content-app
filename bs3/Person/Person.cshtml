@inherits Custom.Hybrid.Razor14

<div class="co-container-outer mb-4 mb-md-5">
  <div class="co-container-inner">
    <div class="row">
      @foreach (var person in AsList(Data)) {
        <div class="col-sm-4 mb-4" itemscope itemtype="http://schema.org/Person" @Edit.TagToolbar(person)>
          @Html.Partial("Part Properties.cshtml", new { data = person })
        </div>
      }
    </div>
  </div>
</div>

@Html.Partial("../shared/Assets.cshtml", new { parts = "scripts,icons"})